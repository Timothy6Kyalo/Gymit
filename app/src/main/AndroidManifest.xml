<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/gym"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gym"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gymit"
        tools:targetApi="31">
        <activity
            android:name=".CLassScheduleActivity"
            android:exported="false"
            android:label="@string/title_activity_class_schedule"
            android:theme="@style/Theme.Gymit" />
        <activity
            android:name=".WorkoutTrackingActivity"
            android:exported="false"
            android:label="@string/title_activity_workout_tracking"
            android:theme="@style/Theme.Gymit" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.Gymit" />
        <activity
            android:name=".RegistrationActivity"
            android:exported="false"
            android:label="@string/title_activity_registration"
            android:theme="@style/Theme.Gymit" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Gymit">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>